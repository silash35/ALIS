generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model places {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId

  userMail String?
  userName String

  name        String
  description String
  address     String
  key         String
  phone       String?
  email       String?
  imageURL    String?
  website     String?
}
